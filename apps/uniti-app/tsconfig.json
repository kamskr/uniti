{
  "extends": [
    "@uniti/tsconfig/base.json"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "react-native",
    "checkJs": false,
    "moduleSuffixes": [
      ".ios",
      ".android",
      ".native",
      ""
    ],
    "target": "es2015",
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "include": [
    "src",
    "*.ts",
    "*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
